[run]
source = app

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    
ignore_errors = True
omit =
    tests/*
    */migrations/*
    */site-packages/*
    .venv/*